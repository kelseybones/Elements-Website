<div class='content'>
  <img src="{{image}}">
  <p class="color">NAME: </p><p>{{name}}</p></br>
  <p class="color">SYMBOL: </p><p>{{small}}</p></br>
  <p class="color">CLASSIFICATION: </p><p>{{category}}</p></br>
  <p class="color">ATOMIC NUMBER: </p><p>{{number}}</p></br>
  <p class="color">ATOMIC MASS: </p><p>{{molar}}</p></br>
  <p class="color">ELECTRON CONFIGURATION: </p><p>{{electrons}}</p></br>
  <p class="color">MELTING POINT: </p><p>{{melting}}</p></br>
  <p class="color">BOILING POINT: </p><p> {{boiling}}</p></br>
  <p class="color">STATE AT 20 DEGREES CELCIUS: </p><p>{{state}}</p></br>
  <p class="color">ISOTOPES: </p><p> {{isotopes}}</p></br></br>
  <table>
    <tr>
      <td>Atomic Radius</td>
      <td>{{atomicradius}}</td>
    </tr>
    <tr>
      <td>Covalent Radius</td>
      <td>{{covalentradius}}</td>
    </tr>
    <tr>
      <td>Electron Affinity</td>
      <td>{{affinity}}</td>
    </tr>
    <tr>
      <td>Electronegativity</td>
      <td>{{electronegativity}}</td>
    </tr>
    <tr>
      <td>Ionisation Energies</td>
      <td>{{ionisation}}</td>
    </tr>
  </table></br>
  <p class="color">Appearance</p></br>
  <p>{{appearance}}</p></br></br>
  <p class="color">History</p></br>
  <p>{{history}}</p></br></br>
</div>
